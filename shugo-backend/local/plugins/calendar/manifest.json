{
  "id": "calendar",
  "name": "Calendar Plugin",
  "version": "1.0.0",
  "description": "Activities and events management for SHUGO",
  "author": "SHUGO Team",
  "license": "MIT",
  "dependencies": [],
  "permissions": [
    "database.write",
    "notifications.send",
    "sync.queue"
